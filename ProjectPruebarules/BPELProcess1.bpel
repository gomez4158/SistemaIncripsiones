  Oracle JDeveloper BPEL Designer 
  
  Created: Wed Mar 04 17:33:16 ART 2015
  Author:  cbergamo
  Type: BPEL 2.0 Process
  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="BPELProcess1"
               targetNamespace="http://xmlns.oracle.com/SistemaDeInscripsiones/ProjectPruebarules/BPELProcess1"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/SistemaDeInscripsiones/ProjectPruebarules/BPELProcess1"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://www.example.org"
         xmlns:ns2="http://xmlns.oracle.com/OracleRules1/OracleRules1_DecisionService_1"
         xmlns:ns3="http://xmlns.oracle.com/bpel"
         xmlns:ns4="http://xmlns.oracle.com/bpel/rules">
  <import namespace="http://www.example.org" location="xsd/elem.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://xmlns.oracle.com/SistemaDeInscripsiones/ProjectPruebarules/BPELProcess1" location="BPELProcess1.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpelprocess1_client" partnerLinkType="client:BPELProcess1" myRole="BPELProcess1Provider"/>
    <partnerLink name="OracleRules1.OracleRules1_DecisionService_1"
                 partnerRole="IDecisionService_Role"
                 partnerLinkType="ns2:IDecisionService_PL"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BPELProcess1RequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BPELProcess1ResponseMessage"/>
    <variable name="VariableEntradaRul" element="ns1:numero"/>
    <variable name="VariableSalidaRul" element="ns1:numero"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPELProcess1.wsdl) -->
    <receive name="receiveInput" partnerLink="bpelprocess1_client" portType="client:BPELProcess1" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="Assign1">
      <copy>
        <from>$inputVariable.payload</from>
        <to>$VariableEntradaRul</to>
      </copy>
    </assign>
    <scope name="Rule1" exitOnStandardFault="no">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:decide"/>
      </bpelx:annotation>
      <variables>
        <variable name="dsIn" messageType="ns2:callFunctionStatelessMessage"/>
        <variable name="dsOut"
                  messageType="ns2:callFunctionStatelessDecisionMessage"/>
      </variables>
      <sequence name="Sequence1">
        <assign name="Facts_To_Rule_Service">
          <copy>
            <from>$VariableEntradaRul</from>
            <to>$dsIn.payload/ns2:parameterList/ns4:integer</to>
          </copy>
        </assign>
        <assign name="BPEL_Header">
          <copy>
            <from>"OracleRules1_DecisionService_1"</from>
            <to variable="dsIn" part="payload">
              <query>@name</query>
            </to>
          </copy>
        </assign>
        <invoke name="Invoke"
                partnerLink="OracleRules1.OracleRules1_DecisionService_1"
                portType="ns2:IDecisionService"
                operation="callFunctionStateless" inputVariable="dsIn"
                outputVariable="dsOut"/>
        <assign name="Rule_Service_To_Facts">
          <copy>
            <from>$dsOut.payload/ns2:resultList/ns4:integer</from>
            <to>$VariableSalidaRul</to>
          </copy>
        </assign>
      </sequence>
    </scope>
    <assign name="Assign2">
      <copy>
        <from>$VariableSalidaRul</from>
        <to>$outputVariable.payload</to>
      </copy>
    </assign>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="bpelprocess1_client" portType="client:BPELProcess1" operation="process" variable="outputVariable"/>
  </sequence>
</process>